{
    "name": "bnstree-bot",
    "version": "1.0.0",
    "description": "Official BnSTree Discord Bot",
    "main": "bot.js",
    "dependencies": {
        "apollo-cache-inmemory": "^1.1.9",
        "apollo-client": "^2.2.5",
        "apollo-link-http": "^1.5.1",
        "discord.js": "^11.3.0",
        "dotenv": "^5.0.0",
        "graphql": "^0.13.1",
        "graphql-tag": "^2.8.0",
        "mongodb": "^3.0.4",
        "node-fetch": "^2.0.0",
        "redis": "^2.8.0",
        "twitter": "^1.7.1",
        "winston": "^3.0.0-rc1"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.2",
        "eslint": "^4.19.0",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1"
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "es6": true
        },
        "extends": "standard",
        "rules": {
            "indent": [
                "warn",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "space-before-function-paren": 0,
            "spaced-comment": 0,
            "no-unused-vars": 1
        }
    },
    "author": "dakaringer"
}
